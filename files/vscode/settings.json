{
    /* Editor Settings */
    "breadcrumbs.enabled": true,
    "editor.accessibilitySupport": "off",
    "editor.fontFamily": "Inconsolata, Consolas, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 16,
    "editor.glyphMargin": false,
    "editor.minimap.enabled": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.suggestSelection": "first",
    "explorer.confirmDelete": false,
    "files.associations": {
        "*.w": "cpp"
    },
    "files.exclude": {
        "*.dll": true,
        "*.exe": true
    },
    "telemetry.enableTelemetry": false,
    "terminal.integrated.fontSize": 16,
    "workbench.colorTheme": "Monokai",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    /* Language agnostic plugins settings */
    "cSpell.checkLimit": 10000,
    "cSpell.language": "en,en-US",
    "cSpell.maxDuplicateProblems": 100,
    "cSpell.maxNumberOfProblems": 5000,
    "git.enableSmartCommit": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    /* Language specific plugins settings */
    "C_Cpp.configurationWarnings": "Disabled",
    "C_Cpp.default.enableConfigurationSquiggles": false,
    "C_Cpp.dimInactiveRegions": false,
    "nmake.selfhost-ring": "Canary",
    "powershell.powerShellDefaultVersion": "Windows PowerShell (x86)",
    "vscode-nmake-tools.showMissingIntellisenseDependencies": true,
    "vscode-nmake-tools.notifyNewFeature": {
        "restoreDependencies": false,
        "buildLite": false,
        "autoImport": false
    },
    "nmake.alwaysInstallNewVersions": false,
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "diffEditor.renderSideBySide": false,
    "extensions.ignoreRecommendations": true,
    "powershell.promptToUpdatePackageManagement": false,
}